{
  "name": "haptic-design-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "typescript": "~4.0.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "yarn test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext js,ts,json",
    "lint:fix": "yarn lint --fix"
  },
  "prettier": {
    "printWidth": 85,
